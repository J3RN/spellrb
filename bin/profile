#!/usr/bin/env ruby

require "bundler/setup"
require "ruby-prof"
require "spell"

RubyProf.start

words = File.read("enable1.txt").split("\n")
spell = Spell::Spell.new(words)

spell.best_match("aligator")

result = RubyProf.stop
printer = RubyProf::GraphHtmlPrinter.new(result)
file_name = "profile/#{Time.now.strftime('%Y%m%d%H%M%S%L')}.html"
printer.print(File.open(file_name, "w+"))

if `uname`.include? "Darwin"
  Kernel.system("open #{file_name}")
end
